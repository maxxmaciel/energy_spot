}

export interface IGenericsData {
    n: string;
    o: IGenericsData[];
    ots: number;
    otsd: number;
    t: stringOrNumber;
    v: number;
    vd: number;
}

export interface IParmsDuplicate {
    value: number | string;
    parm_name: string;
}

export const parmsDuplicate = <T extends { [key: string]: any }>(arr: T[], url_parm: string, field: string, arr_init: IParmsDuplicate[] = []): IParmsDuplicate[] => {
    arr.forEach((value, idx) => {
        arr_init.push({
            value: value[field],
            parm_name: url_parm
        })
        // url_init += url_parm + "=" + value[field];
    });
    return arr_init;
}



